{
  "name": "flipboxdigital/cache",
  "description": "PSR-6 Cache handling",
  "version": "2.0.0",
  "keywords": [
    "cache",
    "psr-6"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Flipbox Factory",
      "email": "hello@flipboxfactory.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "flipboxdigital/http-stream": "^2.0",
    "flipboxdigital/relay": "^2.0",
    "flipboxdigital/skeleton": "^2.0",
    "tedivm/stash": "^0.14.2",
    "psr/log": "^1.0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "squizlabs/php_codesniffer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Flipbox\\Cache\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Flipbox\\Cache\\Tests\\": "tests/src/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.org"
    }
  ]
}